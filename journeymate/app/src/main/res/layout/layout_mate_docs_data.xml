<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/layout_mate_docs_list_data">


    <ImageButton
        android:id="@+id/docs_image"
        android:layout_width="190dp"
        android:layout_height="210dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="140dp"
        android:layout_marginBottom="200dp"
        android:background="@drawable/docs_list_style"
        android:padding="0dp"
        android:scaleType="centerCrop" />

    <TextView
        android:id="@+id/docs_list_title"
        android:layout_width="96dp"
        android:layout_height="46dp"
        android:layout_below="@+id/docs_image"
        android:layout_alignStart="@+id/docs_image"
        android:layout_alignEnd="@+id/docs_image"
        android:layout_marginStart="1dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="10dp"
        android:text="문서 제목"
        android:textAlignment="center"
        android:textSize="24dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/docs_list_created_Date"
        android:layout_width="94dp"
        android:layout_height="36dp"
        android:layout_below="@+id/docs_image"
        android:layout_alignStart="@+id/docs_image"
        android:layout_alignEnd="@+id/docs_image"
        android:layout_marginStart="36dp"
        android:layout_marginTop="147dp"
        android:layout_marginEnd="39dp"
        android:text="2022.01.06"
        android:textAlignment="center"
        android:textSize="21dp"
        android:textStyle="bold" />


</RelativeLayout>